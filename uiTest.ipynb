    "# Basic Python UI window\n",
    "from tkinter import *\n",
    "\n",
    "root = Tk()\n",
    "\n",
    "# Declare a label widget\n",
    "myLabel = Label(root, text=\"Hello World!\")\n",
    "\n",
    "# Packing all the widgets above into one window\n",
    "myLabel.pack()\n",
    "\n",
    "# Loop to make sure the program keep alive\n",
    "root.mainloop()"
   ]
  },
